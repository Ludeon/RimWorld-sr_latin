<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN:
    <li>questDescription->[decreeIntro]\n\nYou must build a monument. It is [monumentSize] in size and will require these resources (total value [monumentRequiredResourcesMarketValue_money]):\n\n[monumentRequiredResources]\n\n[decreeThreatInfo]</li>
  -->
  <Decree_BuildMonument.questDescriptionRules.rulesStrings>
    <li>questDescription->[decreeIntro]\n\nDu musst ein Monument errichten. Es hat eine Größe von [monumentSize] und benötigt folgende Ressourcen (Gesamtwert [monumentRequiredResourcesMarketValue_money]):\n\n[monumentRequiredResources]\n\n[decreeThreatInfo]</li>
  </Decree_BuildMonument.questDescriptionRules.rulesStrings>
  <!-- EN:
    <li>questName->monument decree</li>
  -->
  <Decree_BuildMonument.questNameRules.rulesStrings>
    <li>questName->Erlass: Monument</li>
  </Decree_BuildMonument.questNameRules.rulesStrings>
  <!-- EN: New monument marker arrived -->
  <Decree_BuildMonument.root.nodes.monumentMarkerDestroyed.node.nodes.Letter.label.slateRef>Neue Denkmalmarkierung eingetroffen</Decree_BuildMonument.root.nodes.monumentMarkerDestroyed.node.nodes.Letter.label.slateRef>
  <!-- EN: Since the monument marker has been destroyed, a new one has been sent. -->
  <Decree_BuildMonument.root.nodes.monumentMarkerDestroyed.node.nodes.Letter.text.slateRef>Da die Denkmalmarkierung zerstört wurde, wurde eine neue geschickt.</Decree_BuildMonument.root.nodes.monumentMarkerDestroyed.node.nodes.Letter.text.slateRef>
  
  <!-- EN:
    <li>questDescription->[decreeIntro]\n\nYou must harvest [plantToHarvest_label] x[harvestItemCount].\n\nYou may need to plant and grow the [plantToHarvest_label] first.\n\n[decreeThreatInfo]</li>
  -->
  <Decree_HarvestCrop.questDescriptionRules.rulesStrings>
    <li>questDescription->[decreeIntro]\n\nDu musst [harvestItemCount]x [plantToHarvest_label] ernten.\n\nHierfür musst du eventuell erst einmal [plantToHarvest_label] anpflanzen und wachsen lassen.\n\n[decreeThreatInfo]</li>
  </Decree_HarvestCrop.questDescriptionRules.rulesStrings>
  <!-- EN:
    <li>questName->[plantToHarvest_label] harvest decree</li>
  -->
  <Decree_HarvestCrop.questNameRules.rulesStrings>
    <li>questName->Erlass: Ernte [plantToHarvest_label]</li>
  </Decree_HarvestCrop.questNameRules.rulesStrings>
  
  <!-- EN:
    <li>questDescription->[decreeIntro]\n\nYou must hunt or slaughter [animalToHuntWithCount].\n\n[decreeThreatInfo]</li>
    <li>animalToHuntWithCount(countToHunt==1)->[animalToHunt_indefinite]</li>
    <li>animalToHuntWithCount(countToHunt!=1)->[countToHunt] [animalToHunt_labelPlural]</li>
  -->
  <Decree_HuntAnimal.questDescriptionRules.rulesStrings>
    <li>questDescription->[decreeIntro]\n\nDu musst [animalToHuntWithCount] jagen oder schlachten.\n\n[decreeThreatInfo]</li>
    <li>animalToHuntWithCount(countToHunt==1)->[animalToHunt_indefinite]</li>
    <li>animalToHuntWithCount(countToHunt!=1)->[countToHunt] [animalToHunt_labelPlural]</li>
  </Decree_HuntAnimal.questDescriptionRules.rulesStrings>
  <!-- EN:
    <li>questName->[animalToHunt_label] hunting decree</li>
  -->
  <Decree_HuntAnimal.questNameRules.rulesStrings>
    <li>questName->Erlass: Jage [animalToHunt_label]</li>
  </Decree_HuntAnimal.questNameRules.rulesStrings>
  
  <!-- EN:
    <li>questDescription->[decreeIntro]\n\nYou must produce:\n\n - [thingToProduceLabel]\n\n[decreeThreatInfo]</li>
  -->
  <Decree_ProduceItem.questDescriptionRules.rulesStrings>
    <li>questDescription->[decreeIntro]\n\nDu musst Folgendes herstellen:\n\n - [thingToProduceLabel]\n\n[decreeThreatInfo]</li>
  </Decree_ProduceItem.questDescriptionRules.rulesStrings>
  <!-- EN:
    <li>questName->[defToProduce_label] production decree</li>
  -->
  <Decree_ProduceItem.questNameRules.rulesStrings>
    <li>questName->Erlass: Produziere [defToProduce_label]</li>
  </Decree_ProduceItem.questNameRules.rulesStrings>
  
</LanguageData>