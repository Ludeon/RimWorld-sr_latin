<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: try to get to know -->
  <BuildRapport.label>Kennenlernen</BuildRapport.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] [toldastoryabout] [subject] to [RECIPIENT_nameDef].</li>
